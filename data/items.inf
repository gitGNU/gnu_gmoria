class stacks_with_others_of_this_kind {
      stackable: with_same_kind;
};

class not_stackable {
      stackable: never;
};

class stacks_with_others_of_same_p1 {
      stackable: with_same_p1;
};

class stacks_with_same_p1_and_kind {
      stackable: with_same_p1, with_same_kind;
};

class not_pickupable {
      stackable: never;
};

class scroll: stacks_with_others_of_this_kind {
	   kind: SCROLL;	   letter: "?";      p1: 0;       weight: 5;
	  to_ac: 0;		   to_hit: 0;	 to_dam: 0;	      ac: 0;
	 damage: 0|0;
};

class potion : stacks_with_others_of_this_kind {
	   kind: POTION;	   letter: "!";  weight: 4;
	  to_ac: 0;		   to_hit: 0;	 to_dam: 0;	      ac: 0;
	 damage: 1|1;
};

class edible : stacks_with_others_of_this_kind {
	 letter: ",";
	  to_ac: 0;		   to_hit: 0;	 to_dam: 0;	      ac: 0;
